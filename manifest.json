{
	"name": "Nutsacker",
	"version": "1.0.2",
	"manifest_version": 2,
	"description": "Image improver.",
	"content_scripts": [{
		"matches": [ "http://*/*", "https://*/*", "file://*/*" ],
		"js": ["jquery.min.js", "script.js"]
	}],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Nutsacker"
	},
	"background": {
		"scripts": ["background.js"]
	},
    "web_accessible_resources": ["nutsack.png"]
}